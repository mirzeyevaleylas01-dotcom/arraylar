#include <iostream>
#include <vector>
#include <climits>
#include <algorithm>

int findSmallestPositive(const std::vector<int>& arr) {
    int min_positive = INT_MAX;
    bool found_positive = false;

    for (int num : arr) {
        if (num > 0) {
            found_positive = true;
            if (num < min_positive) {
                min_positive = num;
            }
        }
    }

    if (!found_positive) {
        return -1; // Müsbət ədəd tapılmadıqda -1 qaytarır
    }

    return min_positive;
}

int main() {
    std::vector<int> myArray = {11, 0, -3, 2, 4, -6, 3, -2};
    int smallest = findSmallestPositive(myArray);

    if (smallest != -1) {
        std::cout << "Massivdəki ən kiçik müsbət ədəd: " << smallest << std::endl;
    } else {
        std::cout << "Massivdə müsbət ədəd tapılmadı." << std::endl;
    }

    return 0;
}
